    "consola": "^3.2.3",
    "husky": "^9.0.11",
    "isbot": "^4.1.0",
    "lint-staged": "^15.2.5",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.5",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.6",
    "@graphql-typed-document-node/core": "^3.2.0",
    "@remix-run/dev": "^2.9.2",
